<div class="col-sm-6 mx-auto">
    <form class="form-inline" action="../getWords/WordleService/query">
        <h2 class="text-uppercase text-center text-bold display-1">Get a wordle answer from any day ever</h2>
        <div class="form-group mx-sm-3 mb-2 text-center">
            <label for="date" class="sr-only">Date</label>
            <input type="date" class="form-control" id="date" name="date"
                   aria-describedby="SearchByDate" min="2021-06-19">
        </div>
        <button type="submit" id="submit" class="btn btn-primary mb-2">Submit Date</button>
    </form>
</div>


